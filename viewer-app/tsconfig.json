{
  "extends": "../node_modules/electron-webpack/tsconfig-base.json",
  "compilerOptions": {
    "allowJs": true,
    "module": "commonjs",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": "dist",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "paths": {
      "*": ["node_modules/*"]
    },
    "downlevelIteration": true,
    "lib": ["ESNext", "DOM", "ScriptHost"],
    "strict": true
  },
  "include": ["src/**/*"]
}
