  "version": "0.2.0",
  "configurations": [
    {
      "name": "(Windows) Launch",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/target/debug/starbound-player-editor.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [{ "name": "RUST_BACKTRACE", "value": "1" }],
      "externalConsole": false,
      "requireExactSource": false
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch NSA test",
      "program": "${workspaceFolder}/node-starbound-assets/test.js",
      "cwd": "${workspaceFolder}/node-starbound-assets",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
